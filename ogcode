<script>
function linked () {
   myWindow = window.open("https://touch.www.linkedin.com/#stream", 

"_blank","toolbar=yes,scrollbars=yes,resizable=yes,top=10,left=0,width=500,height=600");
}

function twitter () {
   myWindow = window.open("http://mobile.twitter.com", 

"_blank","toolbar=yes,scrollbars=yes,resizable=yes,top=100,left=200,width=500,height=600");
}


function test2() {
    myWindow = window.open("http://touch.facebook.com", "_blank", 

"toolbar=yes,scrollbars=yes,resizable=yes,top=200,left=100,width=500,height=600");
}
function fbnotif() {
    myWindow = window.open("http://touch.facebook.com/notifications", "_blank", 

"toolbar=yes,scrollbars=yes,resizable=yes,top=200,left=200,width=500,height=600");
}
function test3() {
    myWindow = window.open("http://pinterest.com", "_blank", 

"toolbar=yes,scrollbars=yes,resizable=yes,top=10,left=100,width=800,height=600");
}

function closeWin() {
    myWindow.close();
}
</script>


